{
    "presets": [
        "@babel/preset-env", "@babel/preset-react"
    ],
    "plugins": [
        ["transform-imports", {
        "react-router": {
            "transform": "react-router/${member}",
            "preventFullImport": true
        }
        }]
    ]
    ,
    "env": {
    "production": {
      "plugins": [
        ["transform-react-remove-prop-types", {
          "mode": "wrap",
          "ignoreFilenames": ["node_modules"]
        }]
      ]
    }
  }
}